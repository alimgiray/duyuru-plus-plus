{
  "manifest_version": 3,
  "name": "Duyuru Plus Plus",
  "version": "0.2",
  "content_scripts": [
    {
      "matches": [
        "https://www.eksiduyuru.com/**",
        "https://eksiduyuru.com/**"
      ],
      "js": ["hide.js", "ui.js"],
      "css" : ["style.css"]
    }
  ],
  "permissions": ["storage"],
  "action": {
    "default_icon": {
      "16": "/icons/16.png",
      "32": "/icons/32.png",
      "48": "/icons/48.png",
      "128": "/icons/128.png"
    }
  }
}
